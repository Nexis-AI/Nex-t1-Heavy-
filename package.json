{
  "name": "nex-t1-heavy-multi-agent-system",
  "version": "1.0.0",
  "description": "Elite AI development team simulation modeled after OpenAI and DeepMind workflows",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:agent": "jest --testPathPattern=agents/personas",
    "test:integration": "jest --testPathPattern=integration",
    "test:collaboration": "jest --testPathPattern=collaboration",
    "test:performance": "jest --testPathPattern=performance",
    "eval:agent": "tsx scripts/evaluate-agent.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "docker:build": "docker build -t nex-t1-heavy .",
    "docker:run": "docker run -p 3000:3000 nex-t1-heavy"
  },
  "keywords": [
    "ai",
    "multi-agent",
    "orchestration",
    "typescript",
    "nestjs"
  ],
  "author": "Nexis AI",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/websockets": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/throttler": "^5.1.1",
    "@prisma/client": "^5.9.0",
    "bullmq": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.0",
    "ioredis": "^5.3.2",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "nest-winston": "^1.9.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "prisma": "^5.9.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexisai/nex-t1-heavy-multi-agent-system.git"
  },
  "bugs": {
    "url": "https://github.com/nexisai/nex-t1-heavy-multi-agent-system/issues"
  },
  "homepage": "https://github.com/nexisai/nex-t1-heavy-multi-agent-system#readme"
}